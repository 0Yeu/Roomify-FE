{
  "name": "VLRNBase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android": "npx react-native run-android --appIdSuffix=development",
    "android:staging": "npx react-native run-android --variant=stagingrelease --appIdSuffix=staging",
    "android:release": "npx react-native run-android --variant=release",
    "android-build:staging": "cd android && ./gradlew assembleStagingRelease && cd ..",
    "android-build:release": "cd android && ./gradlew bundleRelease && cd ..",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "start:pure": "npx react-native start --reset-cache",
    "postinstall": "patch-package && cd ios && pod install && cd ..",
    "test": "jest --passWithNoTests",
    "make:splash": "yarn add @bam.tech/react-native-make --dev && npx react-native set-splash --path src/assets/images/App/splash.png --resize cover --background \"#090909\" && yarn remove @bam.tech/react-native-make",
    "make:icon": "yarn add @bam.tech/react-native-make --dev && npx react-native set-icon --platform ios --path src/assets/images/App/iconIos.png && npx react-native set-icon --platform android --path src/assets/images/App/iconAndroid.png && yarn remove @bam.tech/react-native-make",
    "lint": "eslint . --ext .ts,.tsx --fix --max-warnings 0",
    "app:link": "npx react-native link"
  },
